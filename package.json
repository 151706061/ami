{
  "name": "ami.js",
  "version": "v0.0.9-dev",
  "threeVersion": "r81",
  "main": "src/ami.js",
  "keywords": ["ami", "ami.js","three.js", "webgl", "dicom", "nifti", "awesome", "medical", "imaging", "xtk", "nrrd", "vtk", "stl", "trk"],
  "author": {
    "name" : "Nicolas Rannou",
    "email" : "nicolas@eunate.ch",
    "url" : "https://eunate.ch"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://fnndsc.github.io/ami"
  },
  "config" : { 
    "target" : "lessons/00/00.js",
    "transforms" : "-t [babelify --presets [ es2015 ] ] -t glslify"
  },
  "dependencies": {
    "CommonGL": "NicolasRannou/CommonGL#master",
    "dicom-parser": "1.7.3",
    "image-JPEG2000": "OHIF/image-JPEG2000#master",
    "jpeg-lossless-decoder-js": "1.2.3",
    "math-float32-to-binary-string": "^1.0.0",
    "nrrd-js": "^0.2.1",
    "nifti-reader-js": "v0.5.3",
    "pako": "1.0.1"
    },
  "scripts": {
    "//": "Generic tasks",
    "clean:dist": "rm -rf dist/*",
    "clean:dev": "rm -rf dev/*",
    "demo": "node ./scripts/lessons.js --demo",
    "//": "Development tasks https://github.com/npm/npm/issues/7919",
    "dev:lessons": "node ./scripts/lessons.js --dev",
    "dev:examples": "node ./scripts/examples.js --dev",
    "dev:watch": "watchify $npm_package_config_target -d -v $npm_package_config_transforms -o dev/$npm_package_config_target",
    "dev": "npm run dev:lessons && npm run dev:examples && ( live-server dev & npm run dev:watch --ami.js:target=$npm_package_config_target)",
    "//": "Distribution tasks",
    "dist:lessons": "node ./scripts/lessons.js --dist",
    "dist:examples": "node ./scripts/examples.js --dist",
    "dist:buildExamples": "find examples -name '*.js' -print0 | xargs -0 -n1 -I {} bash -c \"echo `date`: Compiling {}; browserify {} -d -v $npm_package_config_transforms | uglifyjs > dist/{} \"",
    "dist:buildAmi": "browserify src/ami.js -d -v $npm_package_config_transforms | uglifyjs > build/ami.js",
    "dist:build": "npm run dist:buildAmi",
    "dist": "npm run clean:dist && npm run dist:lessons && npm run dist:examples && npm run dist:build",
    "//": "Misc tasks",
    "//": "Test",
    "//": "Router for convenience: node run lesson 00 <or> node run example geometries_slice",
    "example": "node ./scripts/router.js examples ",
    "lesson": "node ./scripts/router.js lessons "
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "uglify-js": "^2.7.3",
    "live-server": "^1.1.0",
    "babel-cli": "latest",
    "shelljs": "latest",
    "yarn": "latest",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "7.3.0",
    "browser-sync": "2.7.12",
    "del": "1.2.0",
    "event-stream": "3.3.1",
    "globby": "2.1.0",
    "glslify": "5.1.0",
    "glslify-hex": "2.0.1",
    "gulp": "3.9.0",
    "gulp-eslint": "2.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^2.0.0",
    "gulp-load-plugins": "0.10.0",
    "gulp-rename": "1.2.2",
    "gulp-size": "1.2.2",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "1.2.0",
    "gulp-util": "3.0.6",
    "jsdoc": "jsdoc3/jsdoc#master",
    "minami": "^1.1.1",
    "karma": "0.13.19",
    "karma-browserify": "^4.2.1",
    "karma-jasmine": "~0.2.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "^0.0.20",
    "run-sequence": "1.1.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0",
    "yargs": "3.17.1"
  },
  "engines": {
    "node": ">=5.4.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "glslify"
    ]
  }
}
